{
  "name": "tours",
  "version": "1.0.0",
  "description": "OUTDOORS IS WHERE LIFE HAPPENS",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devServer":"npm-run-all --parallel start watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "start": "live-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kishanjasani/Tour.git"
  },
  "keywords": [
    "tours",
    "travells"
  ],
  "author": "Kishan Jasani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kishanjasani/Tour/issues"
  },
  "homepage": "https://github.com/kishanjasani/Tour#readme",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "concat": "^1.0.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  },
  "dependencies": {
    "live-server": "^1.2.1"
  }
}
